<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="414dp"
        android:layout_height="277dp"
        android:layout_marginBottom="290dp"
        app:layout_constraintBottom_toTopOf="@+id/passRepeat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reg2_logo" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="372dp"
        android:layout_height="198dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView27"
        app:layout_constraintVertical_bias="0.555"
        app:srcCompat="@drawable/about_password" />

    <ImageView
        android:id="@+id/reg4NextButt"
        android:layout_width="371dp"
        android:layout_height="72dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView34"
        app:layout_constraintVertical_bias="0.16"
        app:srcCompat="@drawable/reg2_next" />

    <EditText
        android:id="@+id/passRepeat"
        style="@style/Widget.Material3.AutoCompleteTextView.OutlinedBox.Dense"
        android:layout_width="346dp"
        android:layout_height="48dp"
        android:layout_marginTop="555dp"
        android:elevation="1000dp"
        android:ems="10"
        android:gravity="center|left"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.61" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="362dp"
        android:layout_height="189dp"
        android:layout_marginBottom="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView33"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/reg4_plains" />

    <EditText
        android:id="@+id/passCreate"
        style="@style/Widget.Material3.AutoCompleteTextView.OutlinedBox.Dense"
        android:layout_width="346dp"
        android:layout_height="48dp"
        android:layout_marginBottom="18dp"
        android:elevation="1000dp"
        android:ems="10"
        android:gravity="center|left"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/passRepeat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView33"
        app:layout_constraintVertical_bias="1.0" />


    <EditText
        android:id="@+id/emailreg"
        style="@style/Widget.Material3.AutoCompleteTextView.OutlinedBox.Dense"
        android:layout_width="346dp"
        android:layout_height="48dp"
        android:layout_marginBottom="170dp"
        android:elevation="1000dp"
        android:ems="10"
        android:gravity="center|left"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/imageView33"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editTextText9"
        android:elevation="1000dp"
        style="@style/Widget.Material3.AutoCompleteTextView.OutlinedBox.Dense"
        android:layout_width="346dp"
        android:layout_height="44dp"
        android:layout_marginBottom="110dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/reg4NextButt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView34" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="362dp"
        android:layout_height="97dp"
        android:layout_marginTop="267dp"
        android:layout_marginBottom="359dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView34"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView27"
        app:srcCompat="@drawable/email_panel"/>

    <Button
        android:id="@+id/reg2NextButt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-60dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/reg4NextButt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>